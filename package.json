{
  "name": "image-redactor-service",
  "version": "1.0.0",
  "description": "Production-ready Node.js microservice for image redaction with blur, pixelation, and fill operations",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm --out-dir dist --dts",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "bench:bytes": "scripts/bench-bytes.sh",
    "bench:s3": "scripts/bench-s3.sh"
  },
  "keywords": [
    "image-processing",
    "redaction",
    "blur",
    "pixelate",
    "microservice",
    "fastify",
    "sharp",
    "s3",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "fastify": "^4.28.1",
    "file-type": "^19.5.0",
    "prom-client": "^15.1.3",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
